        private int id;
  private String name;
  private int age;
  private int height;
  private int weight;
  private boolean active;
  private Date dt;

-->
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.apache.ibatis.autoconstructor.SubjectMapper">
    <cache/>
    <sql id="Base_Column_List">`id`,`name`,`age`,`height`,`weight`,`active`,`dt`</sql>
    <resultMap id="BaseResultMap" type="Subject" autoMapping="true">
        <id column="id" jdbcType="INTEGER" property="id"/>
        <result column="name" jdbcType="VARCHAR" property="name"/>
        <result column="age" jdbcType="INTEGER" property="age"/>
        <result column="height" jdbcType="INTEGER" property="height"/>
        <result column="weight" jdbcType="INTEGER" property="weight"/>
        <result column="active" jdbcType="TINYINT" property="active"/>
        <result column="dt" jdbcType="TIMESTAMP" property="dt"/>
       <!-- <discriminator javaType="Object">
            <case value=""></case>
        </discriminator>-->
    </resultMap>
    <select id="selectByPrimaryKey" parameterType="int" resultType="Subject">
        select
          <include refid="Base_Column_List"/>
        from subject
        where id = #{id}
    </select>
</mapper>
